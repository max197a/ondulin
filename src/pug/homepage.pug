  // BEGIN SECTION :: warranty
  section.warranty(data-aos='fade-up')
    .container
      p.warranty__title Сертифицированные подрядчики — гарантия высокого качества сервиса
        .warranty__grid
          .warranty__left
            a.warranty__image(href="#")
              img(src="img/cert.jpg", alt="")
            .warranty__info
              .warranty__info-i
                p.warranty__info-title Виды сертификации
